{
  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|webp|svg)",
      "headers": [
        { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "^(?!www\\.sanital\\.es$).*" }
      ],
      "destination": "https://www.sanital.es/:path*",
      "permanent": true
    }
  ]
}
